            },
            {
              "name": "TxMetadataByPk",
              "query": "query TxMetadataByPk($timepoint_height: Int!, $timepoint_index: Int!, $multisig: String!, $chain: String!) {\n  tx_metadata_by_pk(multisig: $multisig, timepoint_height: $timepoint_height, timepoint_index: $timepoint_index, chain: $chain) {\n    call_data\n    description\n  }\n}\n"
