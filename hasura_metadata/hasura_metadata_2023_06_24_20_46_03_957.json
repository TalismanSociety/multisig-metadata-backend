{
  "resource_version": 47,
  "metadata": {
    "version": 3,
    "sources": [
      {
        "name": "postgres",
        "kind": "postgres",
        "tables": [
          {
            "table": {
              "name": "tx_metadata",
              "schema": "public"
            },
            "insert_permissions": [
              {
                "role": "public",
                "permission": {
                  "check": {},
                  "columns": [
                    "created",
                    "call_data",
                    "multisig",
                    "timepoint_height",
                    "description",
                    "chain",
                    "timepoint_index",
                    "change_config_details"
                  ]
                }
              }
            ],
            "select_permissions": [
              {
                "role": "public",
                "permission": {
                  "columns": [
                    "created",
                    "call_data",
                    "multisig",
                    "timepoint_height",
                    "description",
                    "chain",
                    "timepoint_index",
                    "change_config_details"
                  ],
                  "filter": {}
                }
              }
            ]
          }
        ],
        "configuration": {
          "connection_info": {
            "database_url": {
              "from_env": "PG_DATABASE_URL"
            },
            "isolation_level": "read-committed",
            "use_prepared_statements": false
          }
        }
      }
    ],
    "query_collections": [
      {
        "name": "allowed-queries",
        "definition": {
          "queries": [
            {
              "name": "InsertTxMetadata",
              "query": "    mutation InsertTxMetadata(\n      $timepoint_height: Int!\n      $timepoint_index: Int!\n      $call_data: String!\n      $chain: String!\n      $multisig: String!\n      $description: String!\n      $change_config_details: json\n    ) {\n      insert_tx_metadata(\n        objects: {\n          timepoint_height: $timepoint_height\n          timepoint_index: $timepoint_index\n          call_data: $call_data\n          chain: $chain\n          multisig: $multisig\n          description: $description\n          change_config_details: $change_config_details\n        }\n      ) {\n        affected_rows\n        returning {\n          created\n        }\n      }\n    }"
            },
            {
              "name": "TxMetadataByPk",
              "query": "query TxMetadataByPk($timepoint_height: Int!, $timepoint_index: Int!, $multisig: String!, $chain: String!) {\n  tx_metadata_by_pk(\n    multisig: $multisig\n    timepoint_height: $timepoint_height\n    timepoint_index: $timepoint_index\n    chain: $chain\n  ) {\n    call_data\n    description\n    change_config_details\n  }\n}"
            },
            {
              "name": "AllChangeConfigAttempts",
              "query": "query AllChangeConfigAttempts($multisig: String!, $chain: String!) {\n  tx_metadata(\n    where: {\n      multisig: { _eq: $multisig }\n      chain: { _eq: $chain }\n      change_config_details: { _is_null: false }\n    }\n  ) {\n    change_config_details\n  }\n}\n"
            }
          ]
        }
      }
    ],
    "allowlist": [
      {
        "collection": "allowed-queries",
        "scope": {
          "global": true
        }
      }
    ],
    "backend_configs": {
      "dataconnector": {
        "athena": {
          "uri": "http://data-connector-agent:8081/api/v1/athena"
        },
        "mariadb": {
          "uri": "http://data-connector-agent:8081/api/v1/mariadb"
        },
        "mysql8": {
          "uri": "http://data-connector-agent:8081/api/v1/mysql"
        },
        "oracle": {
          "uri": "http://data-connector-agent:8081/api/v1/oracle"
        },
        "snowflake": {
          "uri": "http://data-connector-agent:8081/api/v1/snowflake"
        }
      }
    }
  }
}